{
  "name": "api-serverless",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=local nodemon",
    "restructure-openapi": "ts-node restructure-openapi.ts",
    "generate": "rm -rf dist && rm -rf tmp_gen_outp && node ./node_modules/@openapitools/openapi-generator-cli/main generate -i openapi.yaml -g typescript -o tmp_gen_outp --additional-properties=modelPropertyNaming=snake_case && rm -rf src/generated && mkdir src/generated && mv tmp_gen_outp/models src/generated/models && rm -rf tmp_gen_outp && mkdir src/generated/http && echo 'export type HttpFile = any;' > src/generated/http/http.ts && rm src/generated/models/all.ts",
    "prebuild": "npm run generate",
    "build": "esbuild src/handler.ts --bundle --sourcemap --platform=node --target=es2020 --outfile=dist/index.js && cp openapi.yaml dist/ && cp openapi.oracle.yaml dist/ && mkdir dist/tmp && cp node_modules/swagger-ui-dist/* dist/tmp && rm dist/tmp/index.js && cp dist/tmp/* dist/ && rm -rf dist/tmp",
    "postbuild": "cd dist && zip -r index.zip *"
  },
  "dependencies": {
    "@aws-sdk/client-managedblockchain": "^3.682.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/express": "^4.17.21",
    "@types/swagger-ui-express": "^4.1.6",
    "@wagmi/chains": "^1.8.0",
    "async-express-decorator": "^1.1.0",
    "aws-serverless-express": "^3.4.0",
    "aws4": "^1.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.2",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mcache": "^0.4.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "serverless-http": "^3.1.1",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-serverless-express": "^3.3.8",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/memory-cache": "^0.2.2",
    "@types/multer": "^1.4.9",
    "@types/node": "^14.14.25",
    "@types/passport": "^1.0.13",
    "@types/passport-anonymous": "^1.0.4",
    "@types/passport-jwt": "^3.0.10",
    "esbuild": "^0.14.11",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "serverless": "^3.27.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "swagger-autogen": "^2.23.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.0.4"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
